{
  "name": "Mixed Solarized Light",
  "type": "light",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#53676D"
    },
    "module": {
      "foreground": "#C25D1E"
    },
  },
  "tokenColors": [
    // ==== General ====
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#DC322F"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment", // comment text
        "punctuation.definition.comment" // comment punctuation, e.g. ';' in ini
      ],
      "settings": {
        "foreground": "#93A1A1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": [
        "keyword",
        "storage", // e.g. 'def'ï¼Œ'nonlocal' in python; 'PRIMARY KEY' in SQL
      ],
      "settings": {
        "foreground": "#8762C6",
      }
    },
    {
      "name": "Operator, Punctuation",
      "scope": [
        "keyword.operator",
        "punctuation",
        "entity.name.function.operator", // e.g. '<<' in cpp
        "constant.other.color", // e.g. '#f0f0f0' in css
      ],
      "settings": {
        "foreground": "#2AA198",
        "fontStyle": "" // make sure it has no font style
      }
    },
    {
      "name": "Escape Characters",
      "scope": [
        "constant.character.escape" // e.g. '\n'
      ],
      "settings": {
        "foreground": "#2AA198",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Suffix",
      "scope": [
        "keyword.other.unit", // e.g. '3.14159f' in c
        "storage.type.numeric" // e.g. '123n' in js
      ],
      "settings": {
        "foreground": "#009C8F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation #", // e.g. '#include'
      "scope": [
        "punctuation.definition.directive.c",
        "punctuation.definition.directive.cpp",
        "punctuation.definition.annotation.java"
      ],
      "settings": {
        "foreground": "#8762C6"
      }
    },
    {
      "name": "Constant",
      "scope": [
        "constant",
        "variable.other.constant", // constant variable
        "support.constant", // e.g. 'sans-serif' in css
        "meta.property-value.css"
      ],
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#C25D1E"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
      ],
      "settings": {
        "foreground": "#489100"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp"
      ],
      "settings": {
        "foreground": "#2AA198"
      }
    },
    {
      "name": "String Placeholder", // e.g. '%d' '%s' in string
      "scope": [
        "constant.character",
        "constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable"
      ],
      "settings": {
        "foreground": "#53676D",
        "fontStyle": "" // make sure it has no font style
      }
    },
    {
      "name": "Variable Language",
      "scope": [
        "variable.language" // e.g. 'this' of 'this.id' in js, '*' of 'java.util.*' in java
      ],
      "settings": {
        "foreground": "#DC322F"
      }
    },
    {
      "name": "Other Variable",
      "scope": [
        "support.other.variable", // e.g. 'console' in js
      ],
      "settings": {
        "foreground": "#CF4B56"
      }
    },
    {
      "name": "Parameter",
      "scope": [
        "variable.parameter",
      ],
      "settings": {
        "foreground": "#657B83",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Label",
      "scope": [
        "entity.name.label"
      ],
      "settings": {
        "foreground": "#53676D",
      }
    },
    {
      "name": "Type, Namespaces",
      "scope": [
        "entity.name.type", // e.g 'React' of 'React.Component' in jsx
        "entity.name.namespace" // e.g. namespace variable in cpp
      ],
      "settings": {
        "foreground": "#B58900"
      }
    },
    {
      "name": "Function",
      "scope": [
        "entity.name.function",
        "variable.function",
      ],
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Support Function",
      "scope": [
        "support.function", // e.g. 'range' in python
        "support.function.magic" // e.g. '__init__' in python
      ],
      "settings": {
        "foreground": "#009C8F",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.type",
        "support.class", // e.g. 'Error' in js
      ],
      "settings": {
        "foreground": "#B58900"
      }
    },
    // ==== Java ====
    {
      "name": "Enum Java",
      "scope": [
        "constant.other.enum.java"
      ],
      "settings": {
        "foreground": "#00978A"
      }
    },
    // ==== Python ====
    {
      "name": "Source",
      "scope": [
        "source.python"
      ],
      "settings": {
        "foreground": "#B58900",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Magic Variable",
      "scope": [
        "support.variable.magic.python" // e.g. '__name__' in python
      ],
      "settings": {
        "foreground": "#53676D",
      }
    },
    {
      "name": "Python Bold String Prefix",
      "scope": [
        "storage.type.string.python" // e.g. make 'f' bold in f-string, 'r' in raw string, 'b' in byte string...
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python @ Decorator",
      "scope": [
        "punctuation.definition.decorator.python", // e.g. '@classmethod', '@decorator', '@property' in python
      ],
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Python 'Self' in Class",
      "scope": "variable.parameter.function.language.special.self.python",
      "settings": {
        "foreground": "#C25D1E",
        "fontStyle": "italic"
      }
    },
    // ==== JavaScript ====
    {
      "name": "JS Support Function",
      "scope": [
        "support.function.js", // e.g. 'fetch()' in js
        "meta.class.js", // e.g. private method name '#encrypt' in js
      ],
      "settings": {
        "foreground": "#268BD2",
      }
    },
    // ==== SQL ====
    {
      "name": "SQL",
      "scope": [
        "source.sql"
      ],
      "settings": {
        "foreground": "#53676D",
      }
    },
    // ==== HTML ====
    {
      "name": "HTML Tag",
      "scope": [
        "entity.name.tag",
      ],
      "settings": {
        "foreground": "#CF4B56"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name",
      ],
      "settings": {
        "foreground": "#6C71C4"
      }
    },
    {
      "name": "HTML Text",
      "scope": [
        "text.html.derivative", // e.g. texts in elements
        "meta.jsx",
        "text.svelte",
      ],
      "settings": {
        "foreground": "#53676D"
      }
    },
    // ==== CSS ====
    {
      "name": "CSS Tag",
      "scope": [
        "entity.name.tag.css",
      ],
      "settings": {
        "foreground": "#CF4B56"
      }
    },
    {
      "name": "CSS Attributes",
      "scope": [
        "entity.other.attribute-name.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#6C71C4"
      }
    },
    {
      "name": "CSS Keyframe",
      "scope": [
        "entity.other.keyframe-offset.css"
      ],
      "settings": {
        "foreground": "#CF4B56"
      }
    },
    {
      "name": "CSS Property Value",
      "scope": [
        "meta.property-value.css" // e.g ...: property-value;
      ],
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    // ==== Markdown ====
    {
      "name": "Markdown Heading Text",
      "scope": [
        "markup.heading.markdown"
      ],
      "settings": {
        "foreground": "#489100",
      }
    },
    {
      "name": "Markdown Heading Punctuation",
      "scope": [
        "punctuation.definition.heading.markdown" // #
      ],
      "settings": {
        "foreground": "#CF4B56"
      }
    },
    {
      "name": "Markdown Text",
      "scope": [
        "text.html.markdown"
      ],
      "settings": {
        "foreground": "#53676D"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": [
        "markup.bold.markdown" // e.g. **bold**, __bold__
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italics", // e.g. *italic*, _italic_
      "scope": [
        "markup.italic.markdown"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown List Punctuation",
      "scope": [
        "punctuation.definition.list.begin.markdown", // '-', '*', '1.'
      ],
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "name": "Markdown Link Title",
      "scope": [
        "string.other.link.title.markdown" // [title](), [x] ..., [^1]
      ],
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": ""
      }
    },
    {
      "name": "Markdown Image Link Title",
      "scope": [
        "string.other.link.description.markdown" // ![title]()
      ],
      "settings": {
        "foreground": "#CF4B56"
      }
    },
    {
      "name": "Markdown Link",
      "scope": [
        "markup.underline.link.markdown", // [](url)
        "markup.underline.link.image.markdown" // ![](url)
      ],
      "settings": {
        "foreground": "#489100",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Markdown Code Block Language Tag",
      "scope": [
        "fenced_code.block.language", // special e.g. ```mermaid
        // "fenced_code.block.language.markdown" // e.g. ```js
      ],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Inline Code",
      "scope": [
        "markup.inline.raw.string.markdown" // e.g. `print("hello")`
      ],
      "settings": {
        "foreground": "#CB4B16"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": [
        "markup.quote.markdown" // > ...
      ],
      "settings": {
        "foreground": "#93A1A1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Horizontal Rule",
      "scope": [
        "meta.separator.markdown" // ---, ***, ___
      ],
      "settings": {
        "foreground": "#93A1A1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Strikethrough",
      "scope": [
        "markup.strikethrough.markdown" // e.g. ~~strikethrough~~
      ],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    // ==== TOML ====
    //
    // No Configurations needed.
    //
    // ==== YAML ====
    {
      "name": "YAML Key",
      "scope": [
        "entity.name.tag.yaml"
      ],
      "settings": {
        "foreground": "#B58900",
      }
    },
    {
      "name": "YAML Ancher & Alias",
      "scope": [
        "entity.name.type.anchor.yaml",
        "variable.other.alias.yaml"
      ],
      "settings": {
        "foreground": "#2AA198",
      }
    },
    {
      "name": "YAML Block Scalar",
      "scope": [
        "keyword.control.flow.block-scalar.folded.yaml", // >
        "keyword.control.flow.block-scalar.literal.yaml" // |
      ],
      "settings": {
        "foreground": "#2AA198",
        "fontStyle": "bold"
      }
    },
    // ==== INI ====
    {
      "name": "INI Title",
      "scope": [
        "entity.name.section.group-title.ini" // e.g. [title]
      ],
      "settings": {
        "foreground": "#B58900",
      }
    },
    {
      "name": "INI Key",
      "scope": [
        "keyword.other.definition.ini"
      ],
      "settings": {
        "foreground": "#B58900",
      }
    },
    {
      "name": "INI Non-string Value",
      "scope": [
        "source.ini",
      ],
      "settings": {
        "foreground": "#CB4B16",
      }
    },
    // ==== XML ====
    {
      "name": "XML Tag",
      "scope": [
        "entity.name.tag.xml",
        "entity.name.tag.localname.xml"
      ],
      "settings": {
        "foreground": "#CF4B56"
      }
    },
    {
      "name": "XML Attributes",
      "scope": [
        "entity.other.attribute-name.xml",
        "entity.other.attribute-name.localname.xml"
      ],
      "settings": {
        "foreground": "#6C71C4"
      }
    },
    {
      "name": "XML Text",
      "scope": [
        "text.xml"
      ],
      "settings": {
        "foreground": "#53676D",
      }
    },
    // ==== Dockerfile ====
    {
      "name": "Dockerfile Value",
      "scope": [
        "source.dockerfile"
      ],
      "settings": {
        "foreground": "#CB4B16",
      }
    },
    // ==== Makefile ====
    {
      "name": "Makefile Value",
      "scope": [
        "source.makefile"
      ],
      "settings": {
        "foreground": "#CB4B16",
      }
    },
    // ==== CMake ====
    {
      "name": "CMake",
      "scope": [
        "source.cmake"
      ],
      "settings": {
        "foreground": "#CB4B16",
      }
    },
    // ==== Nginx Config ====
    {
      "name": "Nginx Text",
      "scope": [
        "meta.context.http.nginx",
        "source.nginx"
      ],
      "settings": {
        "foreground": "#53676D"
      }
    },
    {
      "name": "Nginx Link",
      "scope": [
        "entity.name.context.location.nginx",
        "string.regexp.nginx"
      ],
      "settings": {
        "foreground": "#268BD2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Nginx Path",
      "scope": [
        "meta.context.server.nginx"
      ],
      "settings": {
        "foreground": "#489100",
      }
    },
  ],
